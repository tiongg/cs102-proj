<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<?import javafx.scene.text.Font?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.shape.Rectangle?>
<fx:root xmlns:fx="http://javafx.com/fxml"
         xmlns="http://javafx.com/javafx"
         type="Tab"
         text="Password"
>
    <AnchorPane prefHeight="500.0" prefWidth="782.0">
        <Label text="Create your password" textFill="#a6a6a6"/>

        <HBox spacing="24" style="-fx-border-color: #d7d7d7; -fx-border-radius: 24px;">
            <VBox alignment="CENTER">
                <VBox alignment="TOP_CENTER" style="-fx-border-color: #d9d9d9; -fx-border-radius: 8px;">
                    <Rectangle height="256" strokeWidth="0.0" width="256"/>
                    <VBox.margin>
                        <Insets bottom="8.0" left="16.0" right="16.0" top="16.0"/>
                    </VBox.margin>
                    <padding>
                        <Insets bottom="16.0" left="16.0" right="16.0" top="16.0"/>
                    </padding>
                </VBox>
                <Label text="Teacher"/>
            </VBox>

            <VBox alignment="CENTER_LEFT" fillWidth="false" prefHeight="290.0" prefWidth="417.0"
                  spacing="12" HBox.hgrow="ALWAYS">
                <VBox prefHeight="43.0" prefWidth="400">
                    <Label text="Password"/>
                    <TextField fx:id="passwordInput">
                        <font>
                            <Font name="Inter Regular" size="16.0"/>
                        </font>
                    </TextField>
                </VBox>
                <VBox prefHeight="43.0" prefWidth="400">
                    <Label text="Confirm Password"/>
                    <TextField fx:id="confirmPasswordInput">
                        <font>
                            <Font name="Inter Regular" size="16.0"/>
                        </font>
                    </TextField>
                </VBox>
                <HBox.margin>
                    <Insets bottom="48.0" top="16.0"/>
                </HBox.margin>
            </VBox>
            <padding>
                <Insets bottom="8.0" left="8.0" right="8.0" top="8.0"/>
            </padding>
        </HBox>
    </AnchorPane>
</fx:root>