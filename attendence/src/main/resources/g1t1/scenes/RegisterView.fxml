                      AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0"
                      AnchorPane.topAnchor="0.0">
                    <Label text="Fill in your details" textFill="#a6a6a6"/>

                    <HBox spacing="24" style="-fx-border-color: #d7d7d7; -fx-border-radius: 24px;">
                        <VBox alignment="CENTER">
                            <VBox alignment="TOP_CENTER" style="-fx-border-color: #d9d9d9; -fx-border-radius: 8px;">
                                <Rectangle height="256" strokeWidth="0.0" width="256"/>
                                <VBox.margin>
                                    <Insets bottom="8.0" left="16.0" right="16.0" top="16.0"/>
                                </VBox.margin>
                                <padding>
                                    <Insets bottom="16.0" left="16.0" right="16.0" top="16.0"/>
                                </padding>
                            </VBox>
                            <Label text="Teacher"/>
                        </VBox>

                        <VBox alignment="TOP_CENTER" fillWidth="false" prefHeight="290.0" prefWidth="417.0" spacing="12"
                              HBox.hgrow="ALWAYS">
                            <VBox prefHeight="43.0" prefWidth="400">
                                <Label text="Teacher ID"/>
                                <TextField>
                                    <font>
                                        <Font name="Inter Regular" size="16.0"/>
                                    </font>
                                </TextField>
                            </VBox>
                            <VBox prefHeight="43.0" prefWidth="400">
                                <Label text="Full Name"/>
                                <TextField>
                                    <font>
                                        <Font name="Inter Regular" size="16.0"/>
                                    </font>
                                </TextField>
                            </VBox>
                            <VBox prefHeight="43.0" prefWidth="400">
                                <Label text="Email (Optional)"/>
                                <TextField>
                                    <font>
                                        <Font name="Inter Regular" size="16.0"/>
                                    </font>
                                </TextField>
                            </VBox>
                            <VBox prefHeight="43.0" prefWidth="400">
                                <Label text="Phone (Optional)"/>
                                <TextField>
                                    <font>
                                        <Font name="Inter Regular" size="16.0"/>
                                    </font>
                                </TextField>
                            </VBox>
                            <HBox.margin>
                                <Insets bottom="16.0" top="16.0"/>
                            </HBox.margin>
                        </VBox>
                        <padding>
                            <Insets bottom="8.0" left="8.0" right="8.0" top="8.0"/>
                        </padding>
                    </HBox>
                </VBox>

            </AnchorPane>
        </Tab>
        <Tab text="Password">
            <AnchorPane prefHeight="499.0" prefWidth="782.0">
                <HBox fillHeight="false" layoutX="2.0" layoutY="40.0" maxWidth="-Infinity" prefWidth="780.0"/>
