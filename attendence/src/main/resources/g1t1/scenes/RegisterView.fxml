    <Button layoutX="330.0" layoutY="772.0" mnemonicParsing="false" onMouseClicked="#prev" text="Prev"
            AnchorPane.bottomAnchor="103" AnchorPane.leftAnchor="330.0"/>
    <Button layoutX="1068.0" layoutY="772.0" mnemonicParsing="false" onMouseClicked="#next" text="Next"
            AnchorPane.bottomAnchor="103" AnchorPane.rightAnchor="330.4000000000001"/>
    <StepperControl fx:id="stepper" layoutX="328.0" layoutY="56.0" prefHeight="56.0" prefWidth="780.0"
                    AnchorPane.topAnchor="64.0">
        <labels>
            <FXCollections fx:factory="observableArrayList">
                <String fx:value="Details"/>
                <String fx:value="Password"/>
                <String fx:value="Image Capture"/>
                <String fx:value="Complete"/>
            </FXCollections>
        </labels>
    </StepperControl>
</AnchorPane>
